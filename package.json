{
  "name": "vocabulary-learning-app",
  "version": "1.0.0",
  "description": "A comprehensive vocabulary learning web application with flashcards, quizzes, and spaced repetition",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "vocabulary",
    "flashcard",
    "learning",
    "english"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "cookie-parser": "^1.4.6",
    "sql.js": "^1.10.0",
  "@supabase/supabase-js": "^2.33.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@playwright/test": "^1.49.0"
  }
}
